{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (Functions)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build (functions)",
      "program": "${workspaceFolder}/bin/Debug/net8.0/ex06-project.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": "Attach to .NET Functions",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:azureFunctions.pickProcess}"
    }
  ]
}